{
  "family": "container-ecs",
  "requiresCompatibilities": [
    "FARGATE"
  ],
  "cpu": "512",
  "memory": "1024",
  "networkMode": "awsvpc",
  "taskRoleArn": "arn:aws:iam::790440370975:role/meli-pr-role",
  "executionRoleArn": "arn:aws:iam::790440370975:role/meli-pr-role",
  "containerDefinitions": [
    {
      "portMappings": [
        {
          "protocol": "tcp",
          "containerPort": 9053
        }
      ],
      "cpu": 512,
      "environment": [
        {
          "name": "SERVER_PORT",
          "value": "9053"
        },
        {
          "name": "TBL_REQUEST",
          "value": "XMEN.REQUEST"
        },
        {
          "name": "TBL_AMOUNT",
          "value": "XMEN.AMOUNT"
        },
        {
          "name": "SPRING_PROFILES_ACTIVE",
          "value": "prod"
        }
      ],
      "essential": true,
      "image": "790440370975.dkr.ecr.us-east-1.amazonaws.com/meli-pr-ecs-ecr-mngr:latest",
      "memory": 1024,
      "memoryReservation": 512,
      "name": "container-ecs",
      "logConfiguration": {
        "logDriver": "awslogs",
        "options": {
          "awslogs-region": "us-east-1",
          "awslogs-group": "meli-pr-cw-logs-mngr",
          "awslogs-stream-prefix": "ecs"
        }
      }
    }
  ]
}